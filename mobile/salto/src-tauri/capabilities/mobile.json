{
  "identifier": "mobile-capability",
  "platforms": [
    "android",
    "iOS"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "biometric:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "starknet://**"
        },
        {
          "url": "braavos://**"
        },
        {
          "url": "argent://**"
        }
      ]
    }
  ]
}